{
  "name": "@ilya.sizov/react-datetimepicker",
  "description": "Date range picker with time",
  "version": "0.3.1",
  "license": "GNU",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "keywords": [
    "react",
    "react-daterange",
    "date range picker",
    "date range no bootstrap",
    "ui"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sizovilya/react-datetimepicker"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/lib/utils/*.{js,jsx}",
      "**/src/lib/calendar/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "dependencies": {
    "moment": "2.23.0",
    "prop-types": "15.6.2",
    "react-moment-proptypes": "1.6.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "8.0.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-15": "^1.1.1",
    "react": "^16.7.0 ",
    "react-dom": "^16.7.0 ",
    "react-test-renderer": "^16.7.0 ",
    "react-scripts": "2.1.3"
  }
}
